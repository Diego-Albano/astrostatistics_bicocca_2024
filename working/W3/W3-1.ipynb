    "import numpy as np\n",
    "\n",
    "N = 1000000\n",
    "std = 5\n",
    "\n",
    "\n",
    "def fun(x):\n",
    "    return x**3* np.sqrt(2*np.pi*std**2) /2\n",
    "\n",
    "xi = 2*np.random.normal(0, std, N)\n",
    "\n",
    "integral = np.mean(fun(xi)) - 2*std**4\n",
    "\n",
    "print(integral)"
   ]
  }
