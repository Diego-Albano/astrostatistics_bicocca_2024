    "import numpy as np\n",
    "import scipy.stats\n",
    "\n",
    "N = 10000\n",
    "std = 50\n",
    "\n",
    "\n",
    "gauss = scipy.stats.norm(loc=0, scale=std)\n",
    "xi = np.abs(gauss.rvs(N))\n",
    "\n",
    "I = std*(np.pi/2)**0.5 * np.mean(xi**3)\n",
    "knownresult= 2*std**4 \n",
    "\n",
    "print(I,knownresult, np.abs(I-knownresult)/knownresult)"
   ]
  }
